              <l n="1" xml:id="spp-S032002D-headnote-1-line-group-1-line-1">&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-S032002D-headnote-2">
            <lg n="1" xml:id="spp-S032002D-headnote-2-line-group-1" type="headnote">
