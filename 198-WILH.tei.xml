          <head type="note" n="1" xml:id="spp-198-WILH-headnote-1">
            <lg n="1" xml:id="spp-198-WILH-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-198-WILH-headnote-1-line-group-1-line-1">P<hi rend="small-caps">ART</hi> of it imitated.</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-198-WILH-headnote-2">
            <lg n="1" xml:id="spp-198-WILH-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-198-WILH-headnote-2-line-group-1-line-1"></l>
