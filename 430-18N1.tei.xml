          <head type="note" n="2" xml:id="spp-430-18N1-headnote-2">
            <lg n="1" xml:id="spp-430-18N1-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-430-18N1-headnote-2-line-group-1-line-1"></l>
            </lg>
