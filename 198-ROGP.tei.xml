          <head type="note" n="1" xml:id="spp-198-ROGP-headnote-1">
            <lg n="1" xml:id="spp-198-ROGP-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-198-ROGP-headnote-1-line-group-1-line-1">PART OF IT IMITATED&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-198-ROGP-headnote-2">
            <lg n="1" xml:id="spp-198-ROGP-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-198-ROGP-headnote-2-line-group-1-line-1"></l>
