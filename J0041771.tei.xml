  <text>
    <body>
      <div type="book">
        <linkGrp type="notes"/>
        <div n="J0041771">
          <head type="note" n="1" xml:id="spp-J0041771-headnote-1">
            <lg n="1" xml:id="spp-J0041771-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-J0041771-headnote-1-line-group-1-line-1">How now, sirrah, must I write in a morning to your impudence?&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-J0041771-headnote-2">
            <lg n="1" xml:id="spp-J0041771-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-J0041771-headnote-2-line-group-1-line-1"></l>
            </lg>
          </head>
          <head type="note" n="3" xml:id="spp-J0041771-headnote-3">
            <lg n="1" xml:id="spp-J0041771-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-J0041771-headnote-3-line-group-1-line-1"></l>
            </lg>
          </head>
          <lg n="1">
            <l xml:id="swift-J0041771-line-1" n="1">Stay till night,</l>
            <l xml:id="swift-J0041771-line-2" n="2">And then I'll write</l>
            <l xml:id="swift-J0041771-line-3" n="3">In black and white,</l>
            <l xml:id="swift-J0041771-line-4" n="4">By candle-light</l>
            <l xml:id="swift-J0041771-line-5" n="5">Of wax so bright,</l>
            <l xml:id="swift-J0041771-line-6" n="6">It helps the sight,</l>
            <l xml:id="swift-J0041771-line-7" n="7">A bite a bite────</l>
          </lg>
        </div>
      </div>
    </body>
