            <lg n="1" xml:id="spp-327-314L-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-327-314L-headnote-3-line-group-1-line-1">D<hi rend="superscript">r</hi> Green</l>
            </lg>
