        <div n="609-031Q">
          <head type="note" n="1" xml:id="spp-609-031Q-headnote-1">
            <lg n="1" xml:id="spp-609-031Q-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-609-031Q-headnote-1-line-group-1-line-1">
                <note rend="flush right">Dublin. November 30<hi rend="superscript">th:</hi> 08: 1732.</note>
              </l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-609-031Q-headnote-2">
            <lg n="1" xml:id="spp-609-031Q-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-609-031Q-headnote-2-line-group-1-line-1"></l>
