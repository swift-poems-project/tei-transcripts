        <div n="170-700B">
          <head type="note" n="1" xml:id="spp-170-700B-headnote-1">
            <lg n="1" xml:id="spp-170-700B-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-170-700B-headnote-1-line-group-1-line-1">&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-170-700B-headnote-2">
            <lg n="1" xml:id="spp-170-700B-headnote-2-line-group-1" type="headnote">
