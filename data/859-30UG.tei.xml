          <head type="note" n="4" xml:id="spp-859-30UG-headnote-4">
            <lg n="1" xml:id="spp-859-30UG-headnote-4-line-group-1" type="headnote">
              <l n="1" xml:id="spp-859-30UG-headnote-4-line-group-1-line-1"></l>
            </lg>
