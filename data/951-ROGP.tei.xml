            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-951-ROGP-headnote-2">
            <lg n="1" xml:id="spp-951-ROGP-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-951-ROGP-headnote-2-line-group-1-line-1"></l>
            </lg>
          </head>
          <head type="note" n="3" xml:id="spp-951-ROGP-headnote-3">
            <lg n="1" xml:id="spp-951-ROGP-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-951-ROGP-headnote-3-line-group-1-line-1"></l>
            </lg>
