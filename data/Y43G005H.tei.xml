          <head type="note" n="2" xml:id="spp-Y43G005H-headnote-2">
            <lg n="1" xml:id="spp-Y43G005H-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-Y43G005H-headnote-2-line-group-1-line-1"></l>
            </lg>
