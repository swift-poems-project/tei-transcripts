          <head type="note" n="3" xml:id="spp-X05-002D-headnote-3">
            <lg n="1" xml:id="spp-X05-002D-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-X05-002D-headnote-3-line-group-1-line-1"></l>
