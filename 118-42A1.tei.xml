              <l n="1" xml:id="spp-118-42A1-headnote-1-line-group-1-line-1"><hi rend="italics">By Dean</hi> S<hi rend="small-caps">WIFT</hi>.</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-118-42A1-headnote-2">
            <lg n="1" xml:id="spp-118-42A1-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-118-42A1-headnote-2-line-group-1-line-1"></l>
            </lg>
