          <head type="note" n="1" xml:id="spp-170-490L-headnote-1">
            <lg n="1" xml:id="spp-170-490L-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-170-490L-headnote-1-line-group-1-line-1">Strenuus et Fortis causisq; Philippus &amp;c.&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-170-490L-headnote-2">
            <lg n="1" xml:id="spp-170-490L-headnote-2-line-group-1" type="headnote">
