          <head type="note" n="3" xml:id="spp-916-0253-headnote-3">
            <lg n="1" xml:id="spp-916-0253-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-916-0253-headnote-3-line-group-1-line-1"></l>
