          <head type="note" n="1" xml:id="spp-082-04B2-headnote-1">
            <lg n="1" xml:id="spp-082-04B2-headnote-1-line-group-1" type="headnote">
              <l n="1" xml:id="spp-082-04B2-headnote-1-line-group-1-line-1">He thus characterizes his Lordship, for whom he seems to have had a most rooted contempt, having elsewhere called him “the vainest old fool he ever knew:”&#13;</l>
            </lg>
          </head>
          <head type="note" n="2" xml:id="spp-082-04B2-headnote-2">
            <lg n="1" xml:id="spp-082-04B2-headnote-2-line-group-1" type="headnote">
              <l n="1" xml:id="spp-082-04B2-headnote-2-line-group-1-line-1"></l>
            </lg>
          </head>
          <head type="note" n="3" xml:id="spp-082-04B2-headnote-3">
            <lg n="1" xml:id="spp-082-04B2-headnote-3-line-group-1" type="headnote">
              <l n="1" xml:id="spp-082-04B2-headnote-3-line-group-1-line-1"></l>
            </lg>
          </head>
          <lg n="1">
            <l xml:id="swift-082-04B2-line-1" n="1">om.</l>
            <l xml:id="swift-082-04B2-line-2" n="2">om.</l>
            <l xml:id="swift-082-04B2-line-3" n="3">om.</l>
            <l xml:id="swift-082-04B2-line-4" n="4">om.</l>
            <l xml:id="swift-082-04B2-line-5" n="5">om.</l>
